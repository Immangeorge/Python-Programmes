s=input("value of s")
r=len(s)
a=b=d=0
for c in range(r):
                print(s[c])
                if (s[c])>='A' and (s[c])<='Z':
                             a=a+1
                elif (s[c])>='a' and (s[c])<='z':
                               b=b+1
                elif (s[c])>='0' and (s[c])<='9':
                               d=d+1
print(a)
print(b)
print(d)
