b=[]
for i in range(1,6):
        a=[]
        n=input("Name")
        ag=input("Age")
        sa=input("Salary")
        add=input("Address")
        ph=input("Phone Number")
        a.append(n)
        a.append(ag)
        a.append(sa)
        a.append(add)
        a.append(ph)
        b.append(a)
for i in range(5):
            print(b[i][0],b[i][4])
