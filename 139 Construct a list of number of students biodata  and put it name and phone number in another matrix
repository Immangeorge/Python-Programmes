b=[]
for i in range(1,6):
              a=[]
              n=input("Name")
              ag=input("Age")
              stud=input("Salary")
              ph=input("Phone Number")
              a.append(n)
              a.append(ag)
              a.append(stud)
              a.append(ph)
              b.append(a)
d=[]
for i in range(5):
              c=[]
              c.append(b[i][0])
              c.append(b[i][3])
              d.append(c)
print(d)
